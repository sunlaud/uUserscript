{
  "description": "Minimalistic userscript injector",
  "homepage_url": "https://github.com/sunlaud",
  "manifest_version": 2,
  "name": "uUserscript",
  "options_ui": {
      "page": "options.html",
      "browser_style": true
  },
  "permissions": [
    "<all_urls>",
    "storage",
    "tabs"
  ],
  "version": "1.1",
  "applications": {
      "gecko": {
          "id": "userscript@sunlaud.github.io"
      }
  },
  "background": {
    "scripts": ["background.js"]
  }
}
